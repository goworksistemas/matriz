{
  "name": "Atualizar Comissões - ETL HubSpot → Supabase",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "atualizar_comissoes",
        "responseMode": "responseNode",
        "options": {}
      },
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2.1,
      "position": [
        0,
        0
      ],
      "id": "webhook-comissoes",
      "name": "Webhook",
      "webhookId": "atualizar_comissoes"
    },
    {
      "parameters": {
        "keepOnlySet": true,
        "values": {
          "string": [
            {
              "name": "notebook_path",
              "value": "Central/etl_hubspot_supabase.ipynb"
            },
            {
              "name": "inicio",
              "value": "={{ new Date().toISOString() }}"
            }
          ]
        },
        "options": {}
      },
      "type": "n8n-nodes-base.set",
      "typeVersion": 2,
      "position": [
        240,
        0
      ],
      "id": "definir-notebook",
      "name": "Definir Notebook",
      "alwaysOutputData": true
    },
    {
      "parameters": {
        "command": "=cd /home/jupyter-luiscuba && /opt/tljh/user/bin/jupyter nbconvert --execute --to notebook --inplace {{ $json.notebook_path }} 2>&1",
        "cwd": "/home/jupyter-luiscuba"
      },
      "type": "n8n-nodes-base.ssh",
      "typeVersion": 1,
      "position": [
        480,
        0
      ],
      "id": "executar-notebook",
      "name": "Executar Notebook",
      "alwaysOutputData": true,
      "credentials": {
        "sshPassword": {
          "id": "lGn6P6YfhyNiVzDr",
          "name": "SSH JUPYTER"
        }
      },
      "onError": "continueErrorOutput"
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "leftValue": "={{ $json.stderr }}",
              "rightValue": "",
              "operator": {
                "type": "string",
                "operation": "empty"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [
        720,
        0
      ],
      "id": "verificar-sucesso",
      "name": "Verificar Sucesso"
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={\n  \"status\": \"success\",\n  \"message\": \"✅ ETL executado com sucesso! Dados do HubSpot sincronizados.\",\n  \"detalhes\": {\n    \"notebook\": \"{{ $('Definir Notebook').item.json.notebook_path }}\",\n    \"inicio\": \"{{ $('Definir Notebook').item.json.inicio }}\",\n    \"fim\": \"{{ new Date().toISOString() }}\",\n    \"output\": \"{{ $('Executar Notebook').item.json.stdout || 'Execução concluída' }}\"\n  }\n}",
        "options": {
          "responseHeaders": {
            "entries": [
              {
                "name": "Content-Type",
                "value": "application/json"
              },
              {
                "name": "Access-Control-Allow-Origin",
                "value": "*"
              }
            ]
          }
        }
      },
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1.1,
      "position": [
        960,
        -100
      ],
      "id": "resposta-sucesso",
      "name": "Resposta Sucesso"
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={\n  \"status\": \"error\",\n  \"message\": \"❌ Erro ao executar ETL. Verifique os logs.\",\n  \"detalhes\": {\n    \"notebook\": \"{{ $('Definir Notebook').item.json.notebook_path }}\",\n    \"inicio\": \"{{ $('Definir Notebook').item.json.inicio }}\",\n    \"fim\": \"{{ new Date().toISOString() }}\",\n    \"erro\": \"{{ $('Executar Notebook').item.json.stderr || 'Erro desconhecido' }}\",\n    \"output\": \"{{ $('Executar Notebook').item.json.stdout || '' }}\"\n  }\n}",
        "options": {
          "responseCode": "500",
          "responseHeaders": {
            "entries": [
              {
                "name": "Content-Type",
                "value": "application/json"
              },
              {
                "name": "Access-Control-Allow-Origin",
                "value": "*"
              }
            ]
          }
        }
      },
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1.1,
      "position": [
        960,
        100
      ],
      "id": "resposta-erro",
      "name": "Resposta Erro"
    }
  ],
  "connections": {
    "Webhook": {
      "main": [
        [
          {
            "node": "Definir Notebook",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Definir Notebook": {
      "main": [
        [
          {
            "node": "Executar Notebook",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Executar Notebook": {
      "main": [
        [
          {
            "node": "Verificar Sucesso",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Resposta Erro",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Verificar Sucesso": {
      "main": [
        [
          {
            "node": "Resposta Sucesso",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Resposta Erro",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "pinData": {},
  "meta": {
    "templateCredsSetupCompleted": true
  }
}
